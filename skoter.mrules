				// border-width : 15%
			draw : line

		elseif : $regex("(.*residential)|(.*unclassified)")
			define
				min-zoom : 13
				line-color : white
				line-width : 13:1.8;15:7
				border-style : solid
				border-color : white black 50%
				border-width : 20%
			for : snowmobile 
				define
					line-width : 13:1.4;14:3
			draw : line
			
			define
				line-width : 8:1;12:2
				min-zoom : 8
				max-zoom : 13
				border-style : none
				line-color : white black 25%
			draw : line
			define
				min-zoom : 13
				max-zoom : 16
			draw : text
		elseif : *footway
			define	
				min-zoom : 10
				line-color : #F68474
				line-width : 10:1;12:1.5
				line-style : dot
				border-style : solid
				border-color : white
				border-width : 100%
				border-opacity : 0.3
			draw : line
			draw : text		
		elseif : *cycleway
			define	
				min-zoom : 13
				line-color : blue
				line-width : 1.5
				line-style : dot
				border-style : solid
				border-color : white
				border-width : 100%
				border-opacity : 0.3
			draw : line
		elseif : *bridleway
			define	
				min-zoom : 10
				line-color : #F68474
				line-width : 10:1;12:1.5
				line-style : dot
				border-style : solid
				border-color : white
				border-width : 100%
				border-opacity : 0.3
			draw : line
			draw : text


		//////////////////////////////////////////////////////////////////

		elseif : *snowmobile 
			define 
				line-opacity : 0.90
				
			define	
				min-zoom : 5
				// grön line-color : #4848FF
				line-color : #4040FF
				line-width : 5:1;8:3;12:4;13:6;15:11
				border-style : solid
				border-color : white black 50%
				border-width : 20%

			for : snowmobile=yes
				define
					line-color : #555
			
			for : snowmobile=permissive
				define
					line-color : Violet

			for : snowmobile=private
				define
					line-color : #FE2E2E

			for : toll
				define
				//	line-color : red
					line-color : #2E2EFE
					border-color : #FE2E2E
					border-width : 6:0.2;8:1.75;13:2.5;14:3
					line-width :   5:1  ;8:2.5 ;12:3.4;13:4;15:11

			for : ice_road
				define
					// min-zoom : 11
					line-style : dashdot
					border-style : none
			draw : line

			define
				
				//Från vägar font-size : 11:11;13:10;14:10.3 
				font-size : 11:11;13:12;14:11
				// font-size : 20
				min-zoom : 10
			draw : text



		elseif : *path
			define	
				min-zoom : 10
				line-color : #6E7C6D
				line-width : 10:1;12:1.5
				line-style : dash
				border-style : solid
				border-color : white
				border-width : 100%
				//border-opacity : 0.9
			for : snowmobile
				define
					line-color : #bbb
				
			
			draw : line
			define
				min-zoom : 11
			draw : text

		elseif : *track
