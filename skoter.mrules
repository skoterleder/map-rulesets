				min-zoom : 12
				max-zoom : 14
				text: ref " " name
			draw : text

		elseif : *tertiary
			define
				line-width : 8:1;12:2.3;13:3;16:10
					  // 8:1;12:2.5;13:3;16:10;18:12
			define
				min-zoom : 13
				line-color : #FEFEB2
				border-style : solid
				border-color : #FEFEB2 black 40%
				// border-width : 20%
			draw : line
			define
				min-zoom : 8
				max-zoom : 13
				line-color : white black 25%
				border-style : none
			draw : line
			define
				min-zoom : 12
				max-zoom : 14
			draw : text

		elseif : *service
			define
				min-zoom : 13
				line-color : white
				line-width : 13:1.8;15:7
				border-style : solid
				border-color : #FEFEB2 black 20%
				// border-width : 17%
			for : snowmobile 
				define
					line-width : 13:1.4;14:3
					
			draw : line
			draw : text
			define
				min-zoom : 8
				max-zoom : 13
				line-width : 8:1.5;12:1.5
				border-style : none
				line-color : white black 25%
			draw : line

